//program to compare two strings without using built in function

#include<stdio.h>
#include<string.h>
int main()
{
    int flag=0;
    char s1[30];
    char s2[30];
    printf("enter s1\n");
    gets(s1);
    printf("enter s2\n");
    gets(s2);
    for(int i=0; s1[i]!='\0'||s2[i]!=0; i++)
    {
        if(s1[i]!=s2[i])
        {
         flag=1;
         break;
        }
    }
    if(flag==1)
    {
        printf("not same");
        
    }
    else
    printf("same");
}
