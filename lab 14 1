#include<stdio.h>
#include<string.h>
int main()
{
    char s1[30];
    char s2[30];
    int n, l1, l2;
    printf("enter string 1\n");
    gets(s1);
    printf("enter string 2\n");
    gets(s2);
    printf("enter n");
    scanf("%d", &n);
    l1=strlen(s1);
    l2=strlen(s2);
    printf("li is %d", l1);
    printf("l2 is %d", l2);
    if(l2>n)
    {
        for(int i=0; i<=n; i++)
        {
        
            s1[l1+i]=s2[i];    
        }
       printf("%s\n", s1);
    }
    else{
    for(int i=0; i<=l2; i++)
    {
     s1[l1+i]=s2[i];
    
}
 printf("%s\n", s1);
    }
}
