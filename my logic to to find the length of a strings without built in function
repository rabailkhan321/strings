//program to find the length of a strings without built in function
#include<stdio.h>
#include<string.h>
int main()
{
    int flag=0, l1, l2;
    char s1[30];
    char s2[30];
    printf("enter s1\n");
    gets(s1);
    printf("enter s2\n");
    
    gets(s2);
    
    l1=strlen(s1);
    l2=strlen(s2);
    for(int i=0; i<l1|| i<l2; i++)
    {
        if(s1[i]!=s2[i])
        {
         flag=1;
         break;
        }
    }
    if(flag==1)
    {
        printf("not same");
        
    }
    else
    printf("same");
}
